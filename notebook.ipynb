   "source": [
    "import qrcode\n",
    "from PIL import Image\n",
    "\n",
    "data = \"https://www.youtube.com/\"\n",
    "\n",
    "qr = qrcode.QRCode(version=1, box_size=10, border=5)\n",
    "qr.add_data(data)\n",
    "qr.make(fit=True)\n",
    "\n",
    "image = qr.make_image(fill=\"black\", back_color='white')\n",
    "\n",
    "image.save(\"qr_code.png\")"
   ]
